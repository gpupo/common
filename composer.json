{
    "name": "gpupo/common",
    "description": "Common Objects",
    "homepage": "https://opensource.gpupo.com/common/",
    "authors": [
        {
            "name": "Gilmar Pupo",
            "email": "contact@gpupo.com"
        }
    ],
    "keywords": ["common"],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "symfony/console": "^6.4",
        "symfony/var-dumper": "^6.4",
        "symfony/lock": "^6.4",
        "symfony/dotenv": "^6.4",
        "doctrine/orm": "^2.8",
        "doctrine/persistence": "^2.1",
        "symfony/yaml": "^6.4",
        "symfony/finder": "^6.4",
        "symfony/filesystem": "^6.4",
        "monolog/monolog": "^2.0",
        "symfony/cache": "^6.4",
        "twig/twig": "^2.10|^3.0",
        "psr/simple-cache": "^1.0",
        "phpdocumentor/reflection-docblock": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "Gpupo\\Common\\": "src",
            "Gpupo\\Common\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5||^9.0"
    },
    "bin": ["bin/developer-toolbox"],
    "config": {
        "preferred-install": "dist"
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
